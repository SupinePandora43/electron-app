{
	"name": "electron-app",
	"version": "0.1.0",
	"private": true,
	"main": "src/main.js",
	"homepage": "https://supinepandora43.github.io/electron-app",
	"dependencies": {
		"@material-ui/core": "^4.0.0",
		"@material-ui/icons": "^4.0.0",
		"animejs": "^3.0.1",
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"react-google-charts": "^3.0.11",
		"three": "^0.102.1"
	},
	"scripts": {
		"start": "yarn dev:react",

		"dev": "npm-run-all --parallel dev:*",
		"build": "npm-run-all --parallel build:*",
		"eject": "react-scripts-ts eject",
		"test": "react-scripts-ts test --env=jsdom",
		
		"predeploy": "yarn build",
		"deploy": "gh-pages -d build",

		"dev:react": "react-scripts-ts start",
		"dev:css": "stylus -w src/stylus -o -c src/css",
		"electron": "electron .",

		"build:react": "react-scripts-ts build",
		"build:css": "stylus src/stylus -o src/css -c"
	},
	"devDependencies": {
		"@types/animejs": "^2.0.2",
		"@types/jest": "^24.0.11",
		"@types/node": "^11.11.5",
		"@types/react": "^16.8.8",
		"@types/react-dom": "^16.8.3",
		"electron": "^5.0.2",
		"react-scripts-ts": "3.1.0",
		"stylus": "^0.54.5",
		"typescript": "3.4.5"
	}
}
